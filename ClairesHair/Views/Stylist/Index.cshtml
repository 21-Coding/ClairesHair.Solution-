@{
    Layout = "_Layout";
}

@using Claire.Models;
@model List<Claire.Models.Stylist>;

<h3>Available Stylists:</h3>

@if (@Model.Count == 0)
{
    <p>No Stylists</p>
}
@foreach (Stylists stylist in Model)
{
    <li>@Html.ActionLink($"{stylist.StylistName}", "Details", new { id = stylist.StylistId }): stylist.StylistCell</li>

}

<p>@Html.ActionLink("Add", "Create", "Stylists")</p>
<p>@Html.ActionLink("Main", "Index", "Home")</p>